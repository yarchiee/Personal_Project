{
  "hosting": [
    {
      "target": "github-mockup",
      "public": "build",
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ],
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"]
    },
    {
      "target": "storybook",
      "public": "storybook-static",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"]
    }
  ]
}
